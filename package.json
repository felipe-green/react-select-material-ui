{
  "name": "react-select-material-ui",
  "version": "7.1.0",
  "description": "A react SELECT component based on react-select and looking like a material-ui component",
  "main": "lib/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "rimraf ./lib && tsc",
    "deploy": "cross-env NODE_ENV=production yarn build && yarn publish --non-interactive",
    "test": "jest",
    "storybook:start": "start-storybook -c storybook",
    "storybook:build": "build-storybook -c storybook -o docs",
    "storybook:commit": "git add . && git commit -m \"Updated storybook\"",
    "all": "yarn install && yarn storybook:build && yarn deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iulian-radu-at/react-select-material-ui.git"
  },
  "files": [
    "lib",
    "index.d.ts"
  ],
  "keywords": [
    "react-select",
    "material-ui",
    "react-select-material-ui",
    "react",
    "react-component",
    "select",
    "combobox",
    "input",
    "singleselect",
    "multiselect",
    "multipleselect",
    "tagsselect",
    "colorsselect",
    "ui",
    "material design"
  ],
  "author": "Iulian Radu <iulian.radu@gmx.at>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iulian-radu-at/react-select-material-ui/issues"
  },
  "homepage": "https://github.com/iulian-radu-at/react-select-material-ui#readme",
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "lodash": "^4.17.21",
    "react-select": "^4.3.1"
  },
  "devDependencies": {
    "@storybook/react": "6.3.8",
    "@types/babel-core": "^6.25.7",
    "@types/enzyme": "^3.10.9",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.172",
    "@types/node": "^14.17.14",
    "@types/react": "^16.14.14",
    "@types/react-dom": "^16.9.14",
    "@types/react-select": "^4.0.17",
    "@types/rimraf": "^3.0.2",
    "babel-core": "^6.26.3",
    "babel-jest": "^27.1.0",
    "babel-loader": "^8.2.2",
    "babel-preset-react-app": "^10.0.0",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "jest": "^27.1.0",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.2"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "./node_modules",
      "./history"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "src/.+\\.test\\.tsx$",
    "setupFiles": [
      "./enzymeAdapter.setup.js"
    ]
  }
}
