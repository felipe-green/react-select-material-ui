{
  "name": "react-select-material-ui",
  "version": "9.0.0",
  "description": "A react SELECT component based on react-select and looking like a material-ui component",
  "main": "lib/index.js",
  "sideEffects": false,
  "scripts": {
    "re-install": "rimraf ./node_modules && yarn install",
    "build": "rimraf ./lib && tsc",
    "deploy": "cross-env NODE_ENV=production yarn build && yarn publish --non-interactive",
    "test": "jest",
    "storybook:start": "start-storybook -c storybook -p 9009",
    "storybook:build": "build-storybook -c storybook -o docs",
    "storybook:commit": "git add . && git commit -m \"Updated storybook\"",
    "all": "yarn re-install && yarn storybook:build && yarn deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iulian-radu-at/react-select-material-ui.git"
  },
  "files": [
    "lib",
    "index.d.ts"
  ],
  "keywords": [
    "react-select",
    "material-ui",
    "react-select-material-ui",
    "react",
    "react-component",
    "select",
    "combobox",
    "input",
    "singleselect",
    "multiselect",
    "multipleselect",
    "tagsselect",
    "colorsselect",
    "ui",
    "material design"
  ],
  "author": "Iulian Radu <iulian.radu@gmx.at>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iulian-radu-at/react-select-material-ui/issues"
  },
  "homepage": "https://github.com/iulian-radu-at/react-select-material-ui#readme",
  "dependencies": {
    "@emotion/styled": "11.10.4",
    "@mui/material": "5.10.10",
    "lodash": "4.17.21",
    "react-select": "5.5.4"
  },
  "devDependencies": {
    "@storybook/react": "6.5.12",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/babel-core": "6.25.7",
    "@types/jest": "29.2.0",
    "@types/lodash": "4.14.186",
    "@types/node": "14.18.32",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/rimraf": "3.0.2",
    "babel-core": "6.26.3",
    "babel-jest": "29.2.1",
    "babel-loader": "8.2.5",
    "babel-preset-react-app": "10.0.1",
    "cross-env": "7.0.3",
    "jest": "29.2.1",
    "rimraf": "3.0.2",
    "ts-jest": "29.0.3",
    "typescript": "4.8.4"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "./node_modules",
      "./history"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      ".+\\.js$": "babel-jest",
      ".+\\.tsx?$": "ts-jest"
    },
    "testRegex": "src/.+\\.test\\.tsx$"
  }
}
